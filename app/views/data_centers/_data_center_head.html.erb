<tr>
  <th class="wider-column">Key<br />&nbsp;</th>
  <th class="wider-column">
    ISP
    <i class="fas fa-info-circle small"
       data-toggle="tooltip"
       data-placement="top"
       title="<%= t('definitions.isp_full_name') %>">
    </i>
    <br />&nbsp;
  </th>
  <th class="narrow-column">
    Population
    <i class="fas fa-info-circle small"
       data-toggle="tooltip"
       data-placement="top"
       title="<%= t('definitions.population_isp') %>">
    </i>
    <br />
    <small class="text-muted">(<%= @results[:total_population] %>)</small>
  </th>
  <th class="narrow-column">
    Delinquent
    <i class="fas fa-info-circle small"
       data-toggle="tooltip"
       data-placement="top"
       title="<%= t('definitions.delinquent_isp') %>">
    </i>
    <br />
    <small class="text-muted">
      (<%= @results[:total_delinquent] %> |
      <%= (count_percentage(@results[:total_delinquent], @results[:total_population])) %>%)
    </small>
  </th>
  <th class="wider-column">
    Active Stake
    <i class="fas fa-info-circle small"
       data-toggle="tooltip"
       data-placement="top"
       title="<%= t('definitions.active_stake') %>">
    </i>
    <br />
    <small class="text-muted">
      <%= number_with_delimiter lamports_to_sol(@results[:total_stake]).round(0) %>
    </small>
  </th>
</tr>
