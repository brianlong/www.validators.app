<% @results[:results].each do |k,v| %>
  <tr>
    <td><%= link_to k, data_center_url(network: params[:network], key: k.gsub('/', '-slash-')) %></td>
    <td><%= v[:aso] %></td>
    <td><%= v[:count] %></td>
    <td>
      <%= number_with_delimiter lamports_to_sol(v[:active_stake]).round(0) %>
      (<%= number_to_percentage((v[:active_stake] / @results[:total_stake].to_f)*100.0, precision: 2) %>)
    </td>
  </tr>
<% end %>