<h1><%= params[:network].capitalize %> Data Centers</h1>
<table class='table table-striped table-borderless'>
  <thead class="thead-dark">
    <tr>
      <th>Key</th>
      <th>ISP</th>
      <th>Country</th>
      <th>Location</th>
      <th>Count</th>
    </tr>
  </thead>
  <tbody>
    <% @data_centers.each do |k,v| %>
    <tr>
      <td><%= link_to k, data_center_url(network: params[:network], key: k.gsub('/', '-slash-')) %></td>
      <td><%= v[:aso] %></td>
      <td><%= v[:country] %></td>
      <td><%= v[:locked_at] %></td>
      <td><%= v[:count] %></td>
    </tr>
    <% end %>
  </tbody>


</table>
