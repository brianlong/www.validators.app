<%= javascript_pack_tag 'validators/circulating_supply.js' %>
<%= javascript_pack_tag 'validators/active_stake.js' %>

<% if params[:network] == "mainnet"%>
  <div class="col-md-4 col-sm-6 mb-4">
    <div class="card h-100">
      <div class="card-content">
        <h2 class="h5 card-heading-left">Stake</h2>
        <div id="circulating-supply-stats">
          <circulating-supply/>
        </div>
        <div id="active-stake-stats" class="mt-2">
          <active-stake/>
        </div>
      </div>
    </div>
  </div>
<% end %>
