# Copy this example file to config/cluster.yml for development & testing.
---
mainnet_urls:
  - "https://api.rpcpool.com"
  - "https://api.mainnet-beta.solana.com"
testnet_urls:
  - "https://testnet.solana.com:8899"
rpc_port: 8899

# Both of these need to be x.y.z
software_patch_mainnet: "1.5.8"
software_patch_testnet: "1.5.8"

stake_boss_testnet: stake-boss-pubkey
stake_boss_testnet_keypair_file: stake-boss/file-path.json
